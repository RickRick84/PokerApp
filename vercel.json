{
  "rewrites": [
    { "source": "/PokerApp(/.*)?", "destination": "/PokerApp/index.html" }
  ],
  "builds": [
    {
      "src": "PokerApp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "PokerApp/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/PokerApp/(.*)",
      "dest": "/PokerApp/$1"
    }
  ]
}
