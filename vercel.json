{
  "rewrites": [
    { "source": "/PokerApp(/.*)?", "destination": "/PokerApp/index.html" }
  ],
  "builds": [
    {
      "src": "PokerApp/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/PokerApp/(.*)",
      "dest": "/PokerApp/$1"
    }
  ],
  "framework": null,
  "outputDirectory": "PokerApp/dist",
  "buildCommand": "cd PokerApp && npm install && npm run build"
}
